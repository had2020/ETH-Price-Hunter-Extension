{
    "name": "ETH Price Hunter",
    "description": "ETH Price Tracker",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "coinlogo.png"
    },
    "permissions": [
        "action",
        "storage",
        "activeTab",
        "scripting"
    ]
}